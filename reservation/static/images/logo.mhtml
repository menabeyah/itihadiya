&lt;/head&gt;
&lt;body class=3D"container mt-4"&gt;

    &lt;h1&gt;R=C3=A9server un billet&lt;/h1&gt;

    &lt;form method=3D"post" novalidate&gt;
        {% csrf_token %}
        {{ form.as_p }}

        &lt;!-- Un seul =C3=A9l=C3=A9ment d'affichage dynamique --&gt;
        &lt;p id=3D"places-disponibles" class=3D"text-info font-weight-bold=
"&gt;&lt;/p&gt;

        &lt;button type=3D"submit" class=3D"btn btn-primary"&gt;R=C3=A9serv=
er&lt;/button&gt;
    &lt;/form&gt;

    &lt;a href=3D"{% url 'liste_trajets' %}" class=3D"btn btn-link mt-3"&gt=
;=E2=86=90 Retour =C3=A0 la liste des trajets&lt;/a&gt;

    &lt;!-- JavaScript plac=C3=A9 en bas --&gt;
    &lt;script&gt;
    document.addEventListener('DOMContentLoaded', function () {
        const trajetSelect =3D document.getElementById('id_trajet');
        const placesInfo =3D document.getElementById('places-disponibles');

        function resetColors() {
            placesInfo.classList.remove('text-success', 'text-warning', 'te=
xt-danger', 'text-info');
        }

        trajetSelect.addEventListener('change', function () {
            const trajetId =3D this.value;

            if (trajetId) {
                fetch(`/ajax/places/?trajet_id=3D${trajetId}`)
                    .then(response =3D&gt; response.json())
                    .then(data =3D&gt; {
                        if (data.places_disponibles !=3D=3D undefined) {
                            const places =3D data.places_disponibles;

                            placesInfo.textContent =3D `Places disponibles =
: ${places}`;
                            resetColors();

                            if (places &gt;=3D 5) {
                                placesInfo.classList.add('text-success');  =
// Vert
                            } else if (places &gt;=3D 2) {
                                placesInfo.classList.add('text-warning');  =
// Orange
                            } else {
                                placesInfo.classList.add('text-danger');   =
// Rouge
                            }
                        } else {
                            placesInfo.textContent =3D "Erreur de chargemen=
t.";
                            resetColors();
                            placesInfo.classList.add('text-danger');
                        }
                    })
                    .catch(error =3D&gt; {
                        console.error('Erreur AJAX :', error);
                        placesInfo.textContent =3D "Erreur de connexion.";
                        resetColors();
                        placesInfo.classList.add('text-danger');
                    });
            } else {
                placesInfo.textContent =3D '';
                resetColors();
            }
        });
    });
    &lt;/script&gt;

